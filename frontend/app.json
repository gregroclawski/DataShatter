{
  "expo": {
    "name": "Data Shatter",
    "slug": "data-shatter",
    "owner": "ontract",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "backgroundColor": "#0a0a0f",
      "resizeMode": "contain"
    },
    "scheme": "frontend",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "admobAppId": "ca-app-pub-9692390081647816~7941119756"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0a0a0f"
      },
      "edgeToEdgeEnabled": true,
      "admobAppId": "ca-app-pub-9692390081647816~7941119756"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "backgroundColor": "#0a0a0f"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-dev-client",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9692390081647816~7941119756",
          "iosAppId": "ca-app-pub-9692390081647816~7941119756"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
