\n        } else {\n          // No local data either, start with fresh game\n          console.log('ðŸ†• Starting fresh game for user');\n          setGameState(defaultGameState);\n        }\n      } catch (error) {\n        console.error('âŒ Failed to load local backup:', error);\n        setGameState(defaultGameState);\n      }\n    };\n    const updateZoneProgress = zoneProgress => {\n      setGameState(prev => ({\n        ...prev,\n        zoneProgress\n      }));\n\n      // MILESTONE SAVE: Zone progression is critical - save immediately\n      if (isAuthenticated) {\n        console.log('ðŸ—ºï¸ MILESTONE: Zone progression updated - IMMEDIATE SAVE');\n        saveOnMilestone('zone_progression');\n      }\n    };\n    const updateEquipment = equipment => {\n      setGameState(prev => ({\n        ...prev,\n        equipment\n      }));\n\n      // MILESTONE SAVE: Equipment changes are critical - save immediately\n      if (isAuthenticated) {\n        console.log('âš”ï¸ MILESTONE: Equipment updated - IMMEDIATE SAVE');\n        saveOnMilestone('equipment_update');\n      }\n    };\n    const updateNinja = updates => {\n      // MOBILE DEBUG: Log all updateNinja calls to trace combat progress\n      console.log('ðŸ¥· MOBILE DEBUG - updateNinja CALLED:', {\n        updatesType: typeof updates,\n        timestamp: Date.now(),\n        platform: Platform.default.OS,\n        updates: typeof updates === 'function' ? 'FUNCTION' : updates\n      });\n      setGameState(prev => {\n        console.log('ðŸ¥· MOBILE DEBUG - BEFORE UPDATE:', {\n          currentLevel: prev.ninja.level,\n          currentXP: prev.ninja.experience,\n          currentGold: prev.ninja.gold,\n          currentGems: prev.ninja.gems\n        });\n\n        // Handle both object updates and function updates\n        const actualUpdates = typeof updates === 'function' ? updates(prev.ninja) : updates;\n        console.log('ðŸ¥· MOBILE DEBUG - ACTUAL UPDATES:', actualUpdates);\n        const updatedNinj